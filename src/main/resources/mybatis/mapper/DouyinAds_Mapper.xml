    <insert id="insert" parameterType="huihuang.proxy.ocpx.ads.liangdamao.LiangdamaoAdsDTO">
        <selectKey keyProperty="id" order="AFTER" resultType="java.lang.Integer">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO douyin_ads
        (tp_adv_id, access_id, request_id, imei, imei_md5,
        oaid, oaid_md5, idfa, idfa_md5, advertiser_id, android_id_md5, mac_md5, deep_link,
        ts,os,ip,ip_md5,ua,callback_url,signature,channel_name,extra, create_time, update_time, report_status, callback_status)
        VALUES (#{tp_adv_id}, #{access_id}, #{request_id}, #{imei}, #{imei_md5}, #{oaid}, #{oaid_md5},
        #{idfa}, #{idfa_md5}, #{advertiser_id}, #{android_id_md5}, #{mac_md5}, #{deep_link}, #{ts},
        #{os},#{ip},#{ip_md5},#{ua},#{callback_url},#{signature},#{channelName},#{extra},now(),now(), "0", "0")
    </insert>


    <update id="update" parameterType="huihuang.proxy.ocpx.ads.liangdamao.LiangdamaoAdsDTO">
        UPDATE douyin_ads
        <set>
            <if test="tp_adv_id != null">tp_adv_id = #{tp_adv_id},</if>

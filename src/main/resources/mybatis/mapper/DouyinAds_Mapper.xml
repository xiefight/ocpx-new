            <if test="idfa_md5 != null">idfa_md5 = #{idfa_md5},</if>
            <if test="advertiser_id != null">advertiser_id = #{advertiser_id},</if>
            <if test="android_id_md5 != null">android_id_md5 = #{android_id_md5},</if>
            <if test="mac_md5 != null">mac_md5 = #{mac_md5},</if>
